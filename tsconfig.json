{
    "compilerOptions": {
      "noImplicitAny": false,
      "target": "es2020",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "outDir": "./dist"
    },
    "exclude": [
      "node_modules",
      "undeployed_modules"
    ],
  }